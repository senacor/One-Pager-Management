{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "dist",
        "rootDir": ".",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "exclude": ["node_modules", "dist", "reports", "app_config", "models", "ressources"]
}
