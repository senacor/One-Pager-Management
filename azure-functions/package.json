{
    "name": "",
    "version": "1.0.0",
    "description": "",
    "main": "dist/src/functions/*.js",
    "scripts": {
        "build": "rimraf build && tsc",
        "watch": "tsc -w",
        "clean": "rimraf dist",
        "prestart": "npm run clean && npm run build",
        "start": "func start",
        "ci:windows_ia32": "npm ci --cpu=ia32 --os=win32",
        "test": "jest --silent && npm run cucumber",
        "cucumber:check": "cucumber-js test/features/**/*.feature --dry-run  --require test/features/**/*.ts --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format summary --format progress --format progress-bar",
        "cucumber": "cucumber-js test/features/**/*.feature --require test/features/**/*.ts --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\"  --format html:reports/report.html --format summary --format @cucumber/pretty-formatter --format cucumber-console-formatter --format junit:reports/cucumber-report.xml",
        "snippets": "cucumber-js test/features/**/*.feature --dry-run --require test/features/**/*.ts --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format snippets",
        "steps-usage": "cucumber-js test/features/**/*.feature --dry-run --require test/features/**/*.ts --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format usage",
        "lint": "eslint src test ",
        "lint:fix": "eslint src test --fix",
        "format": "prettier --write \"src/**/*.{js,ts,mjs,cjs,mts,cts}\" \"test/**/*.{js,ts,mjs,cjs,mts,cts}\""
    },
    "dependencies": {
        "@azure/functions": "^4.7.2",
        "@azure/identity": "^4.10.0",
        "@mediapipe/face_detection": "^0.4.1646425229",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@tensorflow-models/face-detection": "^1.0.3",
        "@tensorflow/tfjs": "^4.22.0",
        "@tensorflow/tfjs-backend-wasm": "^4.22.0",
        "@tensorflow/tfjs-converter": "^4.22.0",
        "@tensorflow/tfjs-core": "^4.22.0",
        "franc-min": "^6.2.0",
        "hardened-fetch": "^1.2.3",
        "isomorphic-fetch": "^3.0.0",
        "jszip": "^3.10.1",
        "memfs": "^4.17.2",
        "node-cache": "^5.1.2",
        "sharp": "^0.34.2",
        "template-file": "^6.0.1",
        "ts-node": "^10.9.2",
        "tsuml2": "^0.17.1",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@cucumber/cucumber": "^11.3.0",
        "@cucumber/html-formatter": "^21.11.0",
        "@cucumber/pretty-formatter": "^1.0.1",
        "@eslint/js": "^9.29.0",
        "@microsoft/microsoft-graph-types": "^2.40.0",
        "@types/isomorphic-fetch": "^0.0.39",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.15.21",
        "@types/sinon": "^17.0.4",
        "@types/xml2js": "^0.4.14",
        "babel-jest": "^30.0.0",
        "csv-stringify": "^6.5.2",
        "cucumber-console-formatter": "^1.0.0",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "globals": "^16.2.0",
        "jest": "^30.0.0",
        "jest-junit": "^16.0.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "sinon": "^21.0.0",
        "ts-jest": "^29.4.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.33.1"
    },
    "jest-junit": {
        "outputDirectory": "reports",
        "outputName": "jest-junit.xml",
        "ancestorSeparator": " â€º ",
        "uniqueOutputName": "false",
        "suiteNameTemplate": "{filepath}",
        "classNameTemplate": "{classname}",
        "titleTemplate": "{title}"
    }
}
